{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "routes": [
    {
      "src": "/api/notes",
      "dest": "api/notes.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/api/notes/(.*)",
      "dest": "api/notes.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/api/tasks",
      "dest": "api/tasks.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/api/tasks/(.*)",
      "dest": "api/tasks.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/api/categories",
      "dest": "api/categories.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/api/categories/(.*)",
      "dest": "api/categories.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
